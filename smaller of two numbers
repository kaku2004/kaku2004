/* Write a program to find smaller of two numbers */

#include<stdio.h>
int main()
{
  int a,b;
  printf("Enter the two numbers:");
  scanf("%d%d",&a&b);
  if(a>b)
{
printf("Smaller number is b");
}
else if(a==b)
{
printf("The two numbers are equal");
}
else
{ 
printf("The smaller number is a");
}
return 1;
}
